{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "no-trailing-whitespace": [true, "ignore-blank-lines"],
    "no-console": false,
    "quotemark": [
      true,
      "single"
    ]
  },
  "rulesDirectory": []
}